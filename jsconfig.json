    "baseUrl": ".",
    "paths": {
      "@/src/*": ["./src/*"],
      "@/styles/*": ["./src/styles/*"],
      "@/svg/*": ["./src/assets/svg/*"],
      "@/icons/*": ["./src/assets/icons/*"],
      "@/assets/*": ["./src/assets/*"],
